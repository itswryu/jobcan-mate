{
  "name": "jobcan-mate-webapp",
  "version": "2.0.0",
  "description": "Web application for managing automated Jobcan clock-in/out and other related tasks.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "prod": "NODE_ENV=production pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop ecosystem.config.js",
    "delete": "pm2 delete ecosystem.config.js",
    "logs": "pm2 logs jobcan-mate-webapp",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "jobcan",
    "automation",
    "playwright"
  ],
